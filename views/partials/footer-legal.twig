<div class="footer">
  <div class="container">
    <footer class="footer__main grid">
      <div class="footer__main__location grid__12 grid__8--md">
        <div class="location">
          {% if fn('pll_current_language', 'slug') is same as('fr') %}
            {{options.french_formatted_address}}
          {% else %}
            {{options.english_formatted_address}}
          {% endif %}
        </div>
      </div>
      <div class="footer__main__menu grid__12 grid__4--md">
        {% for item in footer_menu.items %}
          {% if loop.first or loop.index == ((loop.length / 2)|round(0, 'ceil') + 1) %}
            <ul class="nav-menu nav-menu-links">
            {% endif %}
            <li class="menu__item menu-item">
              <a href="{{item.link}}">
                {{item.title}}
              </a>
            </li>
            {% if loop.last or loop.index == ((loop.length / 2)|round(0, 'ceil')) %}
            </ul>
          {% endif %}
        {% endfor %}
      </div>
    </footer>
    <footer class="footer__legal">
      <div class="footer__legal__copyright">
        <p>
          <span class="font-bold">{{fn('pll_e', 'Ontario Place')}}</span>
        - {{fn('pll_e', 'All Rights Reserved &copy; 2017')}}</p>
        <div class="footer__legal__copyright__nav">
          <ul class="nav-menu nav-menu-legal">
            {% for item in legal_menu.items %}
              <li class="menu-item">
                <a href="{{item.link}}">
                  {{item.title}}
                </a>
              </li>
            {% endfor %}
          </ul>
        </div>
      </div>
