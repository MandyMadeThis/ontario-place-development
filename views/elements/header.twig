<!-- /header -->
{% if post._slider %}
  {% set slider_name = fn("get_page_slider", post.get_field("slider")) %}
  {{ function('do_shortcode',"[wise_flexslider slider=#{ slider_name }]") }}
{% endif %}
{% if post_parent and post_parent.thumbnail %}
  <div class="masthead">
    <img src="{{post_parent.thumbnail.src}}" alt="{{post_parent.thumbnail.alt}}"/>
  </div>
{% else %}
  {% if post.thumbnail %}
    <div class="masthead">
      <img src="{{post.thumbnail.src}}" alt="{{post.thumbnail.alt}}"/>
    </div>
  {% endif %}
{% endif %}